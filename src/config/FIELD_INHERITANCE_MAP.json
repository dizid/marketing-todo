  "inheritance_rules": {
    "rule_1": {
      "name": "Circular dependency prevention",
      "description": "If a task is the primary entry point for a field, it should not inherit it back",
      "example": "Audience Research defines targetAudience, so it doesn't inherit it"
    },
    "rule_2": {
      "name": "Override preservation",
      "description": "If user creates task-specific override, preserve it even if source updates",
      "implementation": "Use task_field_overrides table with source tracking"
    },
    "rule_3": {
      "name": "Empty field handling",
      "description": "Show inherited value even if target field is empty",
      "implementation": "Always populate form with inherited value, but allow override"
    },
    "rule_4": {
      "name": "Data type compatibility",
      "description": "Only inherit if data types match or can be converted",
      "example": "Cannot inherit marketingBudget (number) into targetAudience (string)"
    }
  },
